{
  "name": "robin-backend",
  "version": "1.0.0",
  "main": "src/index.ts",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "test": "vitest",
    "db:init": "wrangler d1 execute robin-db --local --file=./schema.sql",
    "db:reset": "wrangler d1 execute robin-db --local --file=./schema.sql",
    "db:query": "wrangler d1 execute robin-db --local",
    "db:remote:init": "wrangler d1 execute robin-db --remote --file=./schema.sql",
    "kv:list": "wrangler kv:key list --binding=ROBIN_CACHE --local"
  },
  "keywords": [
    "cloudflare",
    "workers",
    "video",
    "streaming"
  ],
  "author": "",
  "license": "ISC",
  "description": "Robin Video Platform Backend API",
  "dependencies": {
    "@cloudflare/workers-types": "^4.20251205.0",
    "hono": "^4.10.7",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "tsx": "^4.21.0",
    "wrangler": "^4.53.0"
  }
}
